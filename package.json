{
    "author": {
        "email": "donmahallem@users.noreply.github.com",
        "name": "donmahallem",
        "url": "https://github.com/donmahallem"
    },
    "bugs": {
        "url": "https://github.com/donmahallem/GoogleFitApiTypes/issues"
    },
    "compilerOptions": {
        "baseUrl": "./src/",
        "declaration": true,
        "esModuleInterop": true,
        "lib": [
            "dom",
            "es2017",
            "es6",
            "es7"
        ],
        "module": "commonjs",
        "outDir": "./dist/",
        "strict": true,
        "target": "es5"
    },
    "dependencies": {
        "jsonschema": "~1.2.5"
    },
    "description": "Api Types and helper for Google Fit Api",
    "devDependencies": {
        "@types/chai": "~4.2.10",
        "@types/mocha": "~7.0.2",
        "@types/node": "~13.7.7",
        "@types/sinon": "~7.5.2",
        "chai": "~4.2.0",
        "mocha": "~7.1.0",
        "nodemon": "~2.0.2",
        "nyc": "~15.0.0",
        "sinon": "~9.0.0",
        "ts-node": "~8.6.2",
        "tslint": "~6.0.0",
        "typedoc": "~0.16.11",
        "typescript": "~3.8.3"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "dist/*",
        "src/*"
    ],
    "homepage": "https://donmahallem.github.io/GoogleFitApiTypes/",
    "keywords": [
        "api",
        "types"
    ],
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "name": "@donmahallem/google-fit-api-types",
    "nyc": {
        "all": true,
        "exclude": [
            "**/*.d.ts",
            "src/**/*.spec.ts",
            "typings"
        ],
        "extension": [
            ".ts"
        ],
        "include": [
            "src/**/*.ts",
            "src/*.ts"
        ],
        "instrument": true,
        "reporter": [
            "html",
            "json",
            "lcov",
            "text-summary"
        ],
        "require": [
            "ts-node/register"
        ],
        "sourceMap": true
    },
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/donmahallem/GoogleFitApiTypes.git"
    },
    "scripts": {
        "build": "tsc",
        "build:doc": "node ./node_modules/typedoc/bin/typedoc --out ./tt2/ --mode modules --exclude /**/index ./src/",
        "build:live": "nodemon",
        "docs": "typedoc --options typedoc.json",
        "lint": "tslint -c tslint.json -p tsconfig-lint.json src/**/*.ts",
        "lint-fix": "tslint --fix -c tslint.json -p tsconfig-lint.json src/**/*.ts",
        "start": "npm run build:live",
        "test": "mocha",
        "test-ci": "nyc npm run test",
        "test-ci_old": "nyc ts-mocha -p tsconfig.json -- --opts mocha.opts",
        "test:build": "npm run build",
        "test:docs": "npm run docs",
        "test:lint": "npm run lint",
        "test:unit": "npm run test-ci"
    },
    "types": "dist/index.d.ts",
    "version": "1.2.4"
}
